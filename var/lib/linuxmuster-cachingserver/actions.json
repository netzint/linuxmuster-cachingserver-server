{
    "images": {
        "pattern": "/srv/linbo/images/*/*"
    },
    "dhcp": {
        "pattern": "/etc/dhcp/subnets.conf;/etc/dhcp/devices/#school#.conf",
        "posthook": "systemctl restart isc-dhcp-server"
    },
    "linbo": {
        "pattern": "/etc/rsyncd.conf;/etc/rsyncd.secrets;/root/.ssh/id_rsa;/var/lib/linuxmuster/setup.ini;/srv/linbo/start.conf.*;/srv/linbo/boot/grub/*.cfg;/srv/linbo/linbocmd/*;/etc/linuxmuster/sophomorix/#school#/devices.csv;/srv/linbo/log/*"
    }
}